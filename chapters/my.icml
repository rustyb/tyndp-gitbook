              <PathPointType Anchor="-534.44295 301.04698" LeftDirection="-534.44295 301.04698" RightDirection="-534.44295 301.04698" />
              <PathPointType Anchor="534.44295 301.04698" LeftDirection="534.44295 301.04698" RightDirection="534.44295 301.04698" />
