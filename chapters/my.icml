            <PathPointArray>
              <PathPointType Anchor="-495.78523 -293.55705" LeftDirection="-495.78523 -293.55705" RightDirection="-495.78523 -293.55705" />
              <PathPointType Anchor="-495.78523 293.55705" LeftDirection="-495.78523 293.55705" RightDirection="-495.78523 293.55705" />
              <PathPointType Anchor="495.78523 293.55705" LeftDirection="495.78523 293.55705" RightDirection="495.78523 293.55705" />
              <PathPointType Anchor="495.78523 -293.55705" LeftDirection="495.78523 -293.55705" RightDirection="495.78523 -293.55705" />
            </PathPointArray>
