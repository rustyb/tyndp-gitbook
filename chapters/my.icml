              <PathPointType Anchor="292.10738 120.56376" LeftDirection="292.10738 120.56376" RightDirection="292.10738 120.56376" />
              <PathPointType Anchor="292.10738 -120.56376" LeftDirection="292.10738 -120.56376" RightDirection="292.10738 -120.56376" />
            </PathPointArray>
          </GeometryPathType>
        </PathGeometry>
