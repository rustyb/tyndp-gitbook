          <GeometryPathType PathOpen="false">
            <PathPointArray>
              <PathPointType Anchor="-509.55705 -302.49664" LeftDirection="-509.55705 -302.49664" RightDirection="-509.55705 -302.49664" />
              <PathPointType Anchor="-509.55705 302.49664" LeftDirection="-509.55705 302.49664" RightDirection="-509.55705 302.49664" />
              <PathPointType Anchor="509.55705 302.49664" LeftDirection="509.55705 302.49664" RightDirection="509.55705 302.49664" />
              <PathPointType Anchor="509.55705 -302.49664" LeftDirection="509.55705 -302.49664" RightDirection="509.55705 -302.49664" />
            </PathPointArray>
          </GeometryPathType>
        </PathGeometry>
      </Properties>
