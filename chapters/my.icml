              <PathPointType Anchor="-276.00000 207.75000" LeftDirection="-276.00000 207.75000" RightDirection="-276.00000 207.75000" />
              <PathPointType Anchor="276.00000 207.75000" LeftDirection="276.00000 207.75000" RightDirection="276.00000 207.75000" />
              <PathPointType Anchor="276.00000 -207.75000" LeftDirection="276.00000 -207.75000" RightDirection="276.00000 -207.75000" />
            </PathPointArray>
